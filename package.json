{
  "name": "smc-show-case",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "heroku-postbuild": "npm run build:en",
    // "heroku-postbuild": "npm run build:fr && npm run build:en && npm run build:de",

    "build:fr": "ng build --configuration=production-fr",
    "build:en": "ng build --configuration=production-en",
    "build:de": "ng build --configuration=production-de ",
    "build:dev-en": "ng build --configuration=dev-en --watch",
    "build:dev-fr": "ng build --configuration=dev-fr --watch",
    "build:dev-de": "ng build --configuration=dev-de --watch",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "eslint 'src/**/*.ts'",
    "e2e": "ng e2e",
    "postinstall": "ng build --aot --prod",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:i18n-ssr": "npm run build:client-and-server-bundles-i18n && npm run compile:server",
    "build:i18n-ssr-dev": "npm run build:client-and-server-bundles-i18n-dev && npm run compile:server",
    "build:client-and-server-bundles": "ng build --watch && ng run smc-show-case:server --bundleDependencies all --watch",
    "build:client-and-server-bundles-i18n": "npm run build:fr && npm run build:en && npm run build:de && ng run smc-show-case:server:production",
    "build:client-and-server-bundles-i18n-dev": "npm run build:fr"
  },
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.901.4",
    "@angular/animations": "^9.1.4",
    "@angular/cdk": "^9.2.2",
    "@angular/cli": "^9.1.4",
    "@angular/common": "^9.1.4",
    "@angular/compiler": "^9.1.4",
    "@angular/compiler-cli": "^9.1.4",
    "@angular/core": "^9.1.4",
    "@angular/forms": "^9.1.4",
    "@angular/localize": "^9.1.4",
    "@angular/material": "^9.2.2",
    "@angular/platform-browser": "^9.1.4",
    "@angular/platform-browser-dynamic": "^9.1.4",
    "@angular/platform-server": "^9.1.4",
    "@angular/router": "^9.1.4",
    "@ngrx/effects": "^9.1.1",
    "@ngrx/entity": "^9.1.1",
    "@ngrx/router-store": "^9.1.1",
    "@ngrx/store": "^9.1.1",
    "dotenv": "^8.2.0",
    "intersection-observer": "^0.10.0",
    "rxjs": "^6.5.5",
    "typescript": "^3.8.3",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular/language-service": "^9.1.4",
    "@ngrx/store-devtools": "^9.1.1",
    "@types/express": "^4.17.6",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "codelyzer": "^5.2.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.0.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "lite-server": "^2.5.4",
    "protractor": "~5.4.4",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.10.1",
    "webpack-cli": "^3.3.11"
  }
}

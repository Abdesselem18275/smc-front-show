{
  "name": "smc-show-case",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "heroku-postbuild": "npm run build-i18n:en && npm run build-i18n:fr",
    "build-i18n:fr": "ng build --output-path=dist/fr --aot --prod --base-href /fr/ --i18n-file=src/locale/messages.fr.xlf --i18n-format=xlf --i18n-locale=fr",
    "build-i18n:en": "ng build --output-path=dist/en --aot --prod --base-href /en/ --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --i18n-locale=en",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:es && npm run build-i18n:fr",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "eslint 'src/**/*.ts'",
    "e2e": "ng e2e",
    "postinstall": "ng build --aot --prod"
  },
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-next.0",
    "@angular/cdk": "~8.2.3",
    "@angular/cli": "^8.3.20",
    "@angular/common": "^9.0.0-next.0",
    "@angular/compiler": "^9.0.0-next.0",
    "@angular/compiler-cli": "^9.0.0-next.0",
    "@angular/core": "^9.0.0-next.0",
    "@angular/forms": "^9.0.0-next.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^9.0.0-next.0",
    "@angular/platform-browser-dynamic": "^9.0.0-next.0",
    "@angular/router": "^9.0.0-next.0",
    "@ngrx/effects": "^8.4.0",
    "@ngrx/entity": "^8.4.0",
    "@ngrx/router-store": "^8.4.0",
    "@ngrx/store": "^8.4.0",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^5.2.0",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.7.0",
    "node-sass": "^4.13.1",
    "rxjs": "^6.5.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/language-service": "^9.0.0-next.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.5.3"
  }
}

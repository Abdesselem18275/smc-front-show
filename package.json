{
  "name": "smc-show-case",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "heroku-postbuild": "npm run build:fr && npm run build:en && npm run build:de",
    "build:fr": "ng build --configuration=production-fr",
    "build:en": "ng build --configuration=production-en",
    "build:de": "ng build --configuration=production-de ",
    "build:dev-en": "ng build --configuration=dev-en --watch",
    "build:dev-fr": "ng build --configuration=dev-fr --watch",
    "build:dev-de": "ng build --configuration=dev-de --watch",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "eslint 'src/**/*.ts'",
    "e2e": "ng e2e",
    "postinstall": "ng build --aot --prod",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:i18n-ssr": "npm run build:client-and-server-bundles-i18n && npm run compile:server",
    "build:i18n-ssr-dev": "npm run build:client-and-server-bundles-i18n-dev && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run smc-show-case:server:production --bundleDependencies all",
    "build:client-and-server-bundles-i18n": "npm run build:fr && npm run build:en && npm run build:de && ng run smc-show-case:server:production",
    "build:client-and-server-bundles-i18n-dev": "npm run build:fr"
  },
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-next.0",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.20",
    "@angular/common": "^9.0.0-next.0",
    "@angular/compiler": "^9.0.0-next.0",
    "@angular/compiler-cli": "^9.0.0-next.0",
    "@angular/core": "^9.0.0-next.0",
    "@angular/forms": "^9.0.0-next.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^9.0.0-next.0",
    "@ngrx/effects": "^8.4.0",
    "@ngrx/entity": "^8.4.0",
    "@ngrx/router-store": "^8.4.0",
    "@ngrx/store": "^8.4.0",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^5.2.0",
    "core-js": "^2.5.4",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.7.0",
    "node-sass": "^4.13.1",
    "rxjs": "^6.5.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/language-service": "^9.0.0-next.0",
    "@types/express": "^4.17.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.5.4",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.1.0"
  }
}
